package palindromo;

public class quadrado implements FormaGeometricas {

	private int lado = 0;
	public quadrado() {
	}

	public quadrado(int lado) {
		this.lado = lado;
		areaTotal(lado,lado);

	}

	@Override
	public int areaTotal(int base, int altura) {	
		return base * altura;
	}

	public int getBase() {
		return lado;
	}

	public void setBase(int base) {
		this.lado = base;
	}

	public int getAltura() {
		return lado2;
	}

	public void setAltura(int altura) {
		this.lado2 = altura;
	}

}
