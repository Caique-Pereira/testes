package palindromo;

public class quadrado implements FormaGeometricas {
	
	private int base = 0;
	private int altura = 0;
	
	
	public quadrado(){}
	
	public quadrado(int base, int altura) {
		super();
		this.base = base;
		this.altura = altura;
	}



	@Override
	public Integer areaTotal(int base, int altura) {
		return base * altura;
	}
	
	

	public float getBase() {
		return base;
	}




	public void setBase(float base) {
		this.base = base;
	}




	public float getAltura() {
		return altura;
	}




	public void setAltura(float altura) {
		this.altura = altura;
	}




	

}
