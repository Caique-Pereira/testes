package palindromo;

public class quadrado implements FormaGeometricas {
	
	private int base = 0;
	private int altura = 0;
	
	
	public quadrado(){}
	
	public quadrado(int base, int altura) {
		this.base = base;
		this.altura = altura;
		areaTotal(base,altura);
		
	}


	@Override
	public int areaTotal(int base, int altura) {
		return base * altura;
	}
	
	

	public int getBase() {
		return base;
	}




	public void setBase(int base) {
		this.base = base;
	}




	public int getAltura() {
		return altura;
	}




	public void setAltura(float altura) {
		this.altura = altura;
	}




	

}
